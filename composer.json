{
    "name": "unik-codes/arch-installer",
    "description": "Install Ark project or it's components and dependencies.",
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "UnikCodes\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UnikCodes\\Console\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "illuminate/filesystem": "^7.0",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "5.*"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "ark"
    ]
}
